

<h3>Projects Started so Far</h3>

<table class="table table-striped table-hover">
  <thead>
    <th>Category</th>
    <th>Level</th>
    <th>Difficulty</th>
    <th>Title</th>
    <th>Description</th>
    <th>Participation</th>
  </thead>
  <tbody>
    <% @projects.each do |prj| %>
      <tr>
        <td><%= prj.category.name %></td>
        <td><%= prj.level %></td>
        <td><%= prj.difficulty %></td>
        <td><%= link_to prj.title, project_path(prj) %></td>
        <td><%= prj.tagline %></td>
        <td>
          <% if current_user.contributions.where(:project_id => prj.id).any? %>
            <span class="label label-success">Have Contributed to the project</span>
          <% else %>
            <span class="label label">Not Participated Yet</span>
          <% end %>
        </td>
    <% end %>
  </tbody>
</table>



