
<h6 style="display:inline">Upload Avatar</h6>
<%= f.file_field :avatar, id: 'new-attachment', style: "display:none" %>
<div class="input-append">
  <input id="fileCover" class="input-large" type="text">
  <a class="btn" onClick="$('input[id=new-attachment]').click();">Browse</a>
</div>
<div class="upload" style="display:none">
  <div class="progress progress-striped active">
    <div class="bar" style="width: 0%"></div>
  </div>
</div>





